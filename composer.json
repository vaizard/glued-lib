{
	"name": "vaizard/glued-lib",
	"description": "Common code for Glued microservices.",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"library"
	],
	"require": {
		"php": "^8",
		"ext-bcmath": "*",
		"ext-curl": "*",
		"ext-json": "*",
		"grasmash/yaml-expander": "^3",
		"monolog/monolog": "^3",
		"paragonie/csp-builder": "^v2.9.0",
		"psr/log": "^3",
		"selective/transformer": "^1",
		"slim/slim": "^4",
		"symfony/yaml": "^6",
		"vlucas/phpdotenv": "^5",
		"voku/anti-xss": "^4",
		"web-token/jwt-framework": "^3",
		"symfony/notifier": "^6",
		"symfony/telegram-notifier": "^6.2",
		"symfony/mailer": "^6"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2"
	},
	"autoload": {
		"psr-4": {
			"Glued\\Lib\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true
		}
	}
}
