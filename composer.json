{
	"name": "vaizard/glued-lib",
	"description": "Common code for Glued microservices.",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"library"
	],
	"require": {
		"php": "^8.3",
		"ext-bcmath": "*",
		"ext-curl": "*",
		"ext-gmp": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-yaml": "*",
		"grasmash/yaml-expander": "^3",
		"middlewares/utils": "^4.0",
		"psr/http-server-middleware": "^1.0",
		"php-di/php-di": "^7",
		"monolog/monolog": "^3.8",
		"phpfastcache/phpfastcache": "^9",
		"psr/log": "^3",
		"selective/transformer": "^1",
		"slim/slim": "^4",
		"symfony/yaml": "^6",
		"vlucas/phpdotenv": "^5",
		"voku/anti-xss": "^4",
		"web-token/jwt-framework": "^3",
		"symfony/notifier": "^6",
		"symfony/telegram-notifier": "^6.4",
		"symfony/mailer": "^6",
		"paragonie/csp-builder": "^3",
		"opis/json-schema": "^2.3",
		"ramsey/uuid": "^4.7",
		"stolt/json-merge-patch": "^2.0"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2"
	},
	"autoload": {
		"psr-4": {
			"Glued\\Lib\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true
		}
	}
}
