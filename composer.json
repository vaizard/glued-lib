{
	"name": "vaizard/glued-lib",
	"description": "Arbitrary-precision arithmetic library",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"library"
	],
	"require": {
		"php": "^8.1",
		"ext-bcmath": "*",
		"ext-json": "*",
		"middlewares/csp": "^3",
		"middlewares/trailing-slash": "^2",
		"monolog/monolog": "^2",
		"selective/transformer": "^1",
		"slim/slim": "^4",
		"voku/anti-xss": "^4"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2"
	},
	"autoload": {
		"psr-4": {
			"Glued\\Lib\\": "src/"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true
		}
	}
}
